<GUI onOpen="onOpen" onClose="onClose" onCreate="onCreate">
    <GuiElement profile="newLayer"/>
    <Bitmap profile="dialogFullscreenBg" id="dialogBg"/>

    <GuiElement profile="ads_dialogBg" id="dialogElement">
        <ThreePartBitmap profile="fs25_dialogBgMiddle"/>
        <ThreePartBitmap profile="fs25_dialogBgTop"/>
        <ThreePartBitmap profile="fs25_dialogBgBottom"/>
        
        <GuiElement profile="fs25_dialogContentContainer">

            <GuiElement profile="fs25_menuHeaderPanel" absoluteSizeOffset="0px 0px">
                <Bitmap profile="fs25_menuHeaderIconBg">
                    <Bitmap profile="fs25_menuHeaderIcon" imageSliceId="gui.icon_vehicleDealer_machines" />
                </Bitmap>
                <Text profile="fs25_menuHeaderTitle" id="headerText" text="$l10n_ads_report_header_title"/>
                <RoundCorner profile="fs25_shopMoneyBoxBg" id="moneyBoxBg"/>
                <BoxLayout profile="fs25_shopMoneyBox" id="moneyBox">
                    <Text profile="fs25_shopBalance" text="$l10n_ui_balance" />
                    <Text profile="fs25_shopMoney" id="balanceElement"/>
                </BoxLayout>
            </GuiElement>
            
            <!-- =============================================================== -->
            <!-- REPORT INFO                                                    -->
            <!-- =============================================================== -->
            <GuiElement profile="ads_logInfoContainer">
               <ThreePartBitmap profile="ads_containerFrame"/>
                    <Text profile="ads_headerBoldText" id="reportTitle" text="$l10n_ads_report_header_title" position="30px 60px" size="100% 40px"/>

                    <Text profile="ads_textBold" id="vehicleNameTitle" text="Name" position="30px 100px"/>
                    <Text profile="fs25_textDefault" id="vehicleNameValue" text="Fendt 500" position="250px 100px" textBold="false"/>

                    <Text profile="ads_textBold" id="reportDateTitle" text="Date" position="500px 100px"/>
                    <Text profile="fs25_textDefault" id="reportDateValue" text="01 Aug '01" position="750px 100px"/>

                    <Text profile="ads_textBold" id="vehicleAgeTitle" text="Age" position="970px 100px"/>
                    <Text profile="fs25_textDefault" id="vehicleAgeValue" text="12 months" position="1220px 100px"/>

                    <Text profile="ads_textBold" id="vehicleOperatingHoursTitle" text="Operating Hours" position="30px 60px"/>
                    <Text profile="fs25_textDefault" id="vehicleOperatingHoursValue" text="23.3 h." position="250px 60px"/>

                    <Text profile="ads_textBold" id="inspectionTypeTitle" text="Inspection Type" position="500px 60px"/>
                    <Text profile="fs25_textDefault" id="inspectionTypeValue" text="Comprehensive" position="750px 60px"/>

                    <Text profile="ads_textBold" id="inspectionLocationTitle" text="Location" position="970px 60px"/>
                    <Text profile="fs25_textDefault" id="inspectionLocationValue" text="Dealer" position="1220px 60px"/>

                    <ThreePartBitmap profile="ads_textSeporator" position="-5px 85px" size="1350px 1dp" />

            </GuiElement>

            <!-- =============================================================== -->
            <!-- VEHICLE STATS                                                   -->
            <!-- =============================================================== -->
            <GuiElement profile="ads_reportStatsContainer">
               <ThreePartBitmap profile="ads_containerFrame"/>
                
                <!-- LEFT PART -->
                <Bitmap profile="ads_tableList" id="tableHeaderBox" position="20px -15px" size="380px 50px">
                    <Text profile="ads_tableHeaderLeft" position="10px 0px" size="380px 40.77px" text="Overall Condition Assessment"/>
                    <ThreePartBitmap profile="fs25_lineSeparatorBottom" position="0px 2px" />
                </Bitmap>
                
                <SmoothList profile="ads_tableList" id="overallAssessmentTable" position="20px -75px" size="380px 510px">
                    <ListItem profile="ads_tableListItem" name="overallAssessmentRowTemplate" size="380px 40.77px" >
                        <Text profile="ads_tableRowCellLeft" position="10px 0px" size="250px 40.77px" name="reportTableOverallAssessmentTitle" />
                        <Text profile="ads_tableRowCellRight" position="260px 0px" size="110px 40.77px" name="reportTableOverallAssessmentValue" />
                    </ListItem>
                </SmoothList>

                <!-- MIDDLE PART -->
                <Bitmap profile="ads_tableList" id="tableHeaderBox" position="500px -15px" size="380px 50px">
                    <Text profile="ads_tableHeaderLeft" position="10px 0px" size="380px 40.77px" text="System Condition"/>
                    <ThreePartBitmap profile="fs25_lineSeparatorBottom" position="0px 2px" />
                </Bitmap>
                
                <SmoothList profile="ads_tableList" id="systemConditionTable" position="500px -75px" size="380px 510px">
                    <ListItem profile="ads_tableListItem" name="systemConditionRowTemplate" size="380px 40.77px" >
                        <Text profile="ads_tableRowCellLeft" position="10px 0px" size="190px 40.77px" name="reportTableSystemConditionTitle" />
                        <Text profile="ads_tableRowCellRight" position="200px 0px" size="170px 40.77px" name="reportTableSystemConditionValue" />
                    </ListItem>
                </SmoothList>

                <!-- RIGHT PART -->
                <Bitmap profile="ads_tableList" id="tableHeaderBox" position="980px -15px" size="380px 50px">
                    <Text profile="ads_tableHeaderLeft" position="10px 0px" size="380px 40.77px" text="Suspicious Symptoms"/>
                    <ThreePartBitmap profile="fs25_lineSeparatorBottom" position="0px 2px" />
                </Bitmap>
                
                <SmoothList profile="ads_tableList" id="suspiciousSymptomsTable" position="980px -75px" size="380px 510px">
                    <ListItem profile="ads_tableListItemAlt" name="suspiciousSymptomsRowTemplate" size="380px 40.77px" >
                        <Text profile="ads_tableRowCellLeftAlt" position="20px 0px" size="340px 40.77px" name="reportTableSuspiciousSymptomsTitle" textLayoutMode="scrolling" />
                    </ListItem>
                </SmoothList>
                
            </GuiElement>

            <!-- =============================================================== -->`   
            <!-- BREAKDOWNS AND RECOMMENDATIONS PANEL                            -->
            <!-- =============================================================== -->
            
            <GuiElement profile="ads_reportBreakdownsContainer">
                <ThreePartBitmap profile="ads_containerFrame"/>

                <Bitmap profile="ads_tableList" id="reportBreakdwonsTableHeader" position="20px 0px" size="650px 50px">
                    <Text profile="ads_tableHeaderLeft" position="10px 0px" size="380px 40.77px" text="Detected breakdowns"/>
                    <ThreePartBitmap profile="fs25_lineSeparatorBottom" position="0px 2px" />
                </Bitmap>
                
                <SmoothList profile="ads_tableList" id="breakdownsTable" position="20px -60px" size="650px 123px">
                    <ListItem profile="ads_tableListItemAlt" name="reportBreakdownsRowTemplate" size="650px 30.77px" >
                        <Text profile="ads_tableRowCellLeftAlt" position="10px 0px" size="630px 30.77px" name="reportBreakdownsRow" textLayoutMode="scrolling" />
                    </ListItem>
                </SmoothList>
            </GuiElement>

            <GuiElement profile="ads_reportRecContainer">
                <ThreePartBitmap profile="ads_containerFrame"/>

                <Bitmap profile="ads_tableList" id="recommendationsTableHeader" position="20px 0px" size="650px 50px">
                    <Text profile="ads_tableHeaderLeft" position="10px 0px" size="380px 40.77px" text="Ð¡onclusion and recommendations"/>
                    <ThreePartBitmap profile="fs25_lineSeparatorBottom" position="0px 2px" />
                </Bitmap>
                
                <SmoothList profile="ads_tableList" id="recommendationsTable" position="20px -60px" size="650px 123px">
                    <ListItem profile="ads_tableListItemAlt" name="reportRecRowTemplate" size="650px 30.77px" >
                        <Text profile="ads_tableRowCellLeftAlt" position="10px 0px" size="630px 30.77px" name="reportRecRow" textLayoutMode="scrolling" />
                    </ListItem>
                </SmoothList>
            </GuiElement>
        </GuiElement> 

        <!-- Buttons -->
        <BoxLayout profile="fs25_dialogButtonBox">
            <Button profile="buttonBack" text="$l10n_button_back" onClick="onClickBack" id="backButton"/>
        </BoxLayout>
    </GuiElement>

    <GUIProfiles>
        <Profile name="ads_reportStatsContainer" extends="emptyPanel" with="anchorTopStretchingX">
            <size value="1410px 470px"/>
            <position value="0px -170px"/>
        </Profile>

        <Profile name="ads_reportBreakdownsContainer" extends="emptyPanel" with="anchorTopStretchingX">
            <size value="710px 230px"/>
            <position value="-350px -590px"/>
        </Profile>

        <Profile name="ads_reportRecContainer" extends="emptyPanel" with="anchorTopStretchingX">
            <size value="710px 230px"/>
            <position value="350px -590px"/>
        </Profile>

    </GUIProfiles>
</GUI>
